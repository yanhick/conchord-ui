    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "connect-livereload": "^0.5.3",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.2",
    "gulp-bower": "0.0.13",
    "gulp-browserify": "^0.5.1",
    "gulp-clean": "^0.3.1",
    "gulp-compass": "^2.0.4",
