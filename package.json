{
  "name": "conchord-ui",
  "version": "0.0.0",
  "description": "A UI for the conchord file format",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/babel/bin/babel-node server",
    "test": "jest",
    "build-client": "./node_modules/browserify/bin/cmd.js client/index.js -t babelify --outfile dist/conchord-ui.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "flux"
    ]
  },
  "author": "Yannick Dominguez",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^4.0.0",
    "babelify": "^5.0.3",
    "babel": "^4.4.5",
    "babel-core": "^4.7.16",
    "browserify": "^9.0.3",
    "jest": "^0.1.37",
    "jest-cli": "^0.4.0"
  },
  "dependencies": {
    "express": "^4.12.0",
    "flux": "^2.0.1",
    "react": "^0.12.2"
  }
}
